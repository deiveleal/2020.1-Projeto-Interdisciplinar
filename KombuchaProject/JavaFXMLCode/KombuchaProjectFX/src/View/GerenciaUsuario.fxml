            <ImageView fitHeight="44.0" fitWidth="91.0" layoutX="171.0" layoutY="441.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../../../../Design/ApoenaLogo.png" />
               </image>
            </ImageView>
            <ImageView fitHeight="148.0" fitWidth="255.0" layoutX="89.0" layoutY="11.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../../../../Design/LogoKombucha_edit.png" />
               </image>
            </ImageView>
            <Button fx:id="btVoltar" layoutX="24.0" layoutY="341.0" mnemonicParsing="false" prefHeight="45.0" prefWidth="180.0" text="Voltar">
               <font>
                  <Font name="BigNoodleTitling" size="24.0" />
               </font>
            </Button>
         </children>
      </AnchorPane>
      <Button fx:id="btCadastFunc" alignment="CENTER" contentDisplay="CENTER" layoutX="214.0" layoutY="331.0" mnemonicParsing="false" prefHeight="45.0" prefWidth="380.0" text="Cadastrar Funcionário">
