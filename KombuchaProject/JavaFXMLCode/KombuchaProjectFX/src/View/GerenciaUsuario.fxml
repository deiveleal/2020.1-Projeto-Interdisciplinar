         </rowConstraints>
         <children>
            <Label text="Funcionário Registrado:">
               <font>
                  <Font size="12.0" />
               </font>
            </Label>
            <Label fx:id="lblIdFuncionarioAtivo" prefHeight="20.0" prefWidth="9.0" GridPane.rowIndex="1">
               <font>
                  <Font size="12.0" />
               </font>
            </Label>
